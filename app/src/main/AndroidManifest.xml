<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.MAD.TimeIsNow">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.TimeIsNow">
    <activity android:name=".MainActivity">
        <nav-graph android:value="@navigation/nav_graph_main" />
        <intent-filter android:autoVerify="true">
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            <category android:name="android.intent.category.BROWSABLE" />

            <data
                android:scheme="https"
                android:host="thepresent.page.link" />
        </intent-filter>
    </activity>
    <activity android:name="com.MAD.TimeIsNow.ResetActivity" />
    <activity android:name="com.MAD.TimeIsNow.HomeActivity" />
</application>

</manifest>
